{
  "name": "@ai16z/agent",
  "version": "0.1.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "auto": "node --loader ts-node/esm src/auto.ts"
  },
  "dependencies": {
    "@ai16z/adapter-postgres": "v0.1.4-alpha.3",
    "@ai16z/adapter-sqlite": "v0.1.4-alpha.3",
    "@ai16z/client-direct": "v0.1.4-alpha.3",
    "@ai16z/eliza": "v0.1.4-alpha.3",
    "@ai16z/plugin-bootstrap": "v0.1.4-alpha.3",
    "@ai16z/plugin-node": "v0.1.4-alpha.3",
    "better-sqlite3": "^11.5.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "^1.20.0",
      "canvas": "skip",
      "youtube-dl-exec": "skip",
      "sharp": "skip",
      "puppeteer": "skip"
    }
  },
  "devDependencies": {
    "ts-node": "10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
